Procedure analcreaselect anallc_dbf=dbf()lc_analname=substr(lc_dbf, rat('\',lc_dbf)+1,rat('.',lc_dbf)-rat('\',lc_dbf)-1)use ..\..\test\anal_strcopy to ('..\..\test\anal_'+lc_analname) with cdxuse ('..\..\test\anal_'+lc_analname) alias analappend blankreplace oir1 with '---', tp1 with '---', drg_ext with p_kieli, comment with p_logicappend from (lc_dbf)lc_file= ('anal_'+lc_analname+'.dbf')wait window nowait 'Select '+lc_fileset default to ..\analdo ..\anal\drganaldo ..\anal\analohjereturn